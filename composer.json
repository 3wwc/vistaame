{
  "name": "mage-os/project-community-edition",
  "description": "Community built eCommerce Platform for Growth",
  "type": "project",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "laminas/laminas-dependency-plugin": true,
      "mage-os/*": true,
      "php-http/discovery": true,
      "magento/magento-composer-installer": true,
      "magento/inventory-composer-installer": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "algolia/algoliasearch-magento-2": "^3.16",
    "mage-os/composer-dependency-version-audit-plugin": "1.3.0",
    "mage-os/composer-root-update-plugin": "1.3.0",
    "mage-os/module-bundle-sample-data": "1.3.0",
    "mage-os/module-catalog-rule-sample-data": "1.3.0",
    "mage-os/module-catalog-sample-data": "1.3.0",
    "mage-os/module-cms-sample-data": "1.3.0",
    "mage-os/module-configurable-sample-data": "1.3.0",
    "mage-os/module-customer-sample-data": "1.3.0",
    "mage-os/module-downloadable-sample-data": "1.3.0",
    "mage-os/module-grouped-product-sample-data": "1.3.0",
    "mage-os/module-msrp-sample-data": "1.3.0",
    "mage-os/module-offline-shipping-sample-data": "1.3.0",
    "mage-os/module-product-links-sample-data": "1.3.0",
    "mage-os/module-review-sample-data": "1.3.0",
    "mage-os/module-sales-rule-sample-data": "1.3.0",
    "mage-os/module-sales-sample-data": "1.3.0",
    "mage-os/module-swatches-sample-data": "1.3.0",
    "mage-os/module-tax-sample-data": "1.3.0",
    "mage-os/module-theme-sample-data": "1.3.0",
    "mage-os/module-widget-sample-data": "1.3.0",
    "mage-os/module-wishlist-sample-data": "1.3.0",
    "mage-os/product-community-edition": "1.3.0",
    "mage-os/sample-data-media": "1.3.0",
    "melhorenvio/magento2": "dev-main",
    "mercadopago/adb-payment": "^1.11",
    "rafaelstz/traducao_magento2_pt_br": "dev-master",
    "swissup/breeze-blank": "^2.11",
    "swissup/module-marketplace": "^1.10"
  },
  "require-dev": {
    "mage-os/magento-allure-phpunit": "1.3.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7 || ^1.0",
    "dg/bypass-finals": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.22",
    "carlos-mg89/oauth": "^0.8.17",
    "mage-os/magento-coding-standard": "1.3.0",
    "mage-os/magento2-functional-testing-framework": "1.3.0",
    "pdepend/pdepend": "3.x-dev",
    "phpmd/phpmd": "3.x-dev",
    "phpstan/phpstan": "^1.9",
    "phpunit/phpunit": "^10.5",
    "symfony/finder": "^6.4"
  },
  "conflict": {
    "gene/bluefoot": "*"
  },
  "extra": {
    "magento-force": "override"
  },
  "autoload": {
    "psr-4": {
      "Magento\\Framework\\": "lib/internal/Magento/Framework/",
      "Magento\\Setup\\": "setup/src/Magento/Setup/",
      "Magento\\": "app/code/Magento/"
    },
    "psr-0": {
      "": [
        "app/code/",
        "generated/code/"
      ]
    },
    "files": [
      "app/etc/NonComposerComponentRegistration.php"
    ],
    "exclude-from-classmap": [
      "**/dev/**",
      "**/update/**",
      "*/*/Test/**/*Test"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
      "Magento\\Tools\\": "dev/tools/Magento/Tools/",
      "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
      "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
      "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
      "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "version": "1.3.0",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.mage-os.org/"
    }
  ]
}
